<?xml version="1.0" ?>
<layout>
    <itdelight_first_index_index translate="label">
        <label>Contact Us Form</label>
        <reference name="head">
            <action method="setTitle">
                <title>Account Dashboard</title>
            </action>
            <action method="addCss">
                <stylesheet>css/customStyles.css</stylesheet>
            </action>
            <action method="addJs">
                <script>lib/jquery/jquery-1.10.2.min.js</script>
            </action>
            <action method="addJs">
                <script>lib/jquery/noconflict.js</script>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>js/customScript.js</name>
            </action>
        </reference>
        <reference name="root">
            <action method="setTemplate">
                <template>page/2columns-right.phtml</template>
            </action>
        </reference>
        <reference name="content">
            <block type="itdelight_first/test" template="first/index.phtml"/>
        </reference>
    </itdelight_first_index_index>

    <catalog_product_view>
        <reference name="head">
            <action method="addCss"><stylesheet>css/customStyles.css</stylesheet></action>
        </reference>
    </catalog_product_view>
</layout>