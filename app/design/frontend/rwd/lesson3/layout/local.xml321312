            <action method="addCss"><stylesheet>css/customStyles.css</stylesheet></action>
            <action method="addJs"><script>lib/jquery/jquery-1.10.2.min.js</script></action>
            <action method="addJs"><script>lib/jquery/noconflict.js</script></action>
            <action method="addItem"><type>skin_js</type><name>js/customScript.js</name></action>
